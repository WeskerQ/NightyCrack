        </div>
        <script>
            // Helpers
            function throttle(cb, delay = 1000) {
                let shouldWait = false
                let waitingArgs
                const timeoutFunc = () => {
                    if (waitingArgs == null) {
                    shouldWait = false
                    } else {
                    cb(...waitingArgs)
                    waitingArgs = null
                    setTimeout(timeoutFunc, delay)
                    }
                }

                return (...args) => {
                    if (shouldWait) {
                    waitingArgs = args
                    return
                    }

                    cb(...args)
                    shouldWait = true
                    setTimeout(timeoutFunc, delay)
                }
            }

            function debounce(func, timeout = 300){
                let timer;
                return (...args) => {
                    clearTimeout(timer);
                    timer = setTimeout(() => { func.apply(this, args); }, timeout);
                };
            }
        </script>


        <script>
            // Community script
            const communityBoxEl = document.getElementById('community-box'),
             communitySwitcherEl = document.getElementById('community-switcher'),
             communityContentEl = document.getElementById('community-content'),
             communityThemesList = document.getElementById('community-themes-list'),
             communityScriptsList = document.getElementById('community-scripts-list'),
             communityLanguagesList = document.getElementById('community-languages-list'),
             allCommunityTabButtons = document.querySelectorAll('.community__tab-button'),
             allCommunitySearchInputs = Array.from(document.querySelectorAll('[data-community-search]')),
             allCommunityThemesPreviewSetEls = Array.from(document.querySelectorAll('[data-community-themes-preview-setting]')),
             allCommunitySearchInputResetButtons = Array.from(document.querySelectorAll('[data-community-search-input-reset]')),
             allCustomCbInputs = Array.from(document.querySelectorAll('.custom-cb-input')),
             allSelectEls = Array.from(document.querySelectorAll('.tab__select')),
             disableImageModeCb = document.getElementById('disable-image-cb'),
             disableEmbedModeCb = document.getElementById('disable-embed-cb'),
             disablePreviewAutoHideCb = document.getElementById('disable-preview-auto-hide'),
             communityThemesPreviewEl = document.getElementById('community-themes-preview'),
             communtiyThemePreviewImg = document.getElementById('community-theme-preview-img'),
             communityScriptsPreviewCodeEl = document.getElementById('community-script-code-preview'),
             switchPreviewToImageButton = document.getElementById('community-switch-preview-to-image'),
             switchPreviewToEmbedButton = document.getElementById('community-switch-preview-to-embed'),
             allCommunitySelectEls = Array.from(document.querySelectorAll('[data-community-hover-preview]'));

             const getCurrentTab = () => communityBoxEl.dataset.communityTabSelected;

            //  Scripts data, script code will be stored here. MAP key(theme name) -> value (script code)
            const scriptsData = new Map();

            async function handleCommunityTabSwitch(e) {
                const buttonClicked = e.target.closest(".btn-community");
                if(!buttonClicked) return;

                const currentTab = getCurrentTab();

                // prevent switching the same tab
                if(buttonClicked.getAttribute('aria-controls') === currentTab) return;

                // update aria expanded
                allCommunityTabButtons.forEach(button => button.setAttribute('aria-expanded', (button === buttonClicked) ? true : false));

                // leaving animation
                document.getElementById(currentTab).style.opacity = 0;
                await sleep(300)
                
                // update top box data attr
                const selectedTab = buttonClicked.getAttribute('aria-controls');
                communityBoxEl.dataset.communityTabSelected = selectedTab;
                
                // opening animation
                await sleep(25)
                document.getElementById(selectedTab).style.opacity = 1;
            }

            communitySwitcherEl.addEventListener('click', handleCommunityTabSwitch);

            // searchbar

            let lastScrollPosition = 0,
                lastScrollHeight,
                lastTab;
            const MIN_SCROLL_TO_BEGIN = 80;
            const SCROLL_CHECK_DELAY = 100;
            const SCROLL_THRESHOLD = 40;

            const updateLastPosition = throttle(position => lastScrollPosition = position, SCROLL_CHECK_DELAY);

            const toggleSearchBar = (searchBarEl, visible) => {
                if(visible) searchBarEl.classList.remove('hidden')
                else searchBarEl.classList.add('hidden')
            }

            function handleCommunityTabScroll(e) {
                const el = e.target;
                const currentTab = getCurrentTab()
                const searchBarEl = el.querySelector('.tab__search');

                if(lastTab === undefined) lastTab = currentTab;

                if(lastTab !== currentTab) {
                    lastScrollPosition = 0;
                    lastScrollHeight = undefined;
                    lastTab = currentTab
                }

                if(lastScrollHeight === undefined) lastScrollHeight = el.scrollHeight;
                
                if(el.scrollTop < MIN_SCROLL_TO_BEGIN) {
                    updateLastPosition(el.scrollTop)
                    if(el.classList.contains('hidden')) toggleSearchBar(searchBarEl, true);
                    return;
                }
                
                if(lastScrollHeight !== el.scrollHeight) return;

                const scrollDirection = (el.scrollTop > lastScrollPosition) ? 'DOWN' : 'UP';
                
                if (scrollDirection === 'DOWN') {
                    toggleSearchBar(searchBarEl, false)
                } else if(scrollDirection === 'UP' && el.scrollTop < lastScrollPosition - SCROLL_THRESHOLD) {
                    toggleSearchBar(searchBarEl, true)
                }
                
                updateLastPosition(el.scrollTop)

                lastScrollHeight = el.scrollHeight;
            }

            allSelectEls.forEach(el=>el.addEventListener('scroll', handleCommunityTabScroll))

            // Preview on hover
            const allElsToHide = Array.from(document.querySelectorAll('.tab__preview-box--themes .hide-when-preview-enabled'));

            let lastActiveElement,
                pointerHovering = false, // Whether pointer is hovering on an important item (li/img)
                previewMode = 'both', // both, embed, image
                disablePreviewAutoHide = false;
                pointerType = 'mouse'; // mouse/keyboard
                ;

            const HIDE_AFTER_MS = 8000;

            const hideThemeDebounce = debounce(() => {hideThemePreview()}, 1000)

            async function hideThemePreview() {
                if(disablePreviewAutoHide || pointerType === 'keyboard') return;

                if(communityThemesPreviewEl.dataset.communityPreviewEnabled === "false") return;

                if(pointerHovering) {
                    setTimeout(hideThemeDebounce, HIDE_AFTER_MS)
                    return;
                }
                
                // Fade out img
                communtiyThemePreviewImg.style.opacity = 0;
                await sleep(310);
                communityThemesPreviewEl.dataset.communityPreviewEnabled = false;
                lastActiveElement.dataset.communityCurrentlyPreviewing = false;
                
                // Fade in svg and text
                allElsToHide.forEach(el => el.style.display = 'block');
                await sleep(50);
                allElsToHide.forEach(el => el.style.opacity = 1);
                await sleep(310)

                // reset last active element
                lastActiveElement = null;
            }

            async function initializeThemePreview(tempPreviewMode = null) {
                const imageModeUrl = lastActiveElement.querySelector('.theme-input-preview .image')?.src || null;
                const embedModeUrl = lastActiveElement.querySelector('.theme-input-preview .embed')?.src || null;

                const embedModeSelected = (tempPreviewMode === 'embed') || (!tempPreviewMode && previewMode === 'embed');

                communtiyThemePreviewImg.src = (embedModeSelected) ? embedModeUrl : imageModeUrl;
                switchPreviewToEmbedButton.disabled = (embedModeSelected || !embedModeUrl) ? true : false;
                switchPreviewToImageButton.disabled = (embedModeSelected || !imageModeUrl) ? false : true;

                if(communityThemesPreviewEl.dataset.communityPreviewEnabled === "false") {
                    // Fade out svg and text
                    allElsToHide.forEach(el => el.style.opacity = 0);
                    await sleep(310);
                    allElsToHide.forEach(el => el.style.display = 'none');
                    
                    // Fade in preview img
                    communityThemesPreviewEl.dataset.communityPreviewEnabled = true;
                    await sleep(50);
                    communtiyThemePreviewImg.style.opacity = 1;
                }

                setTimeout(hideThemeDebounce, HIDE_AFTER_MS)
            }

            // on preview buttons
            switchPreviewToEmbedButton.addEventListener('click', () => {
                initializeThemePreview('embed')
            })
            switchPreviewToImageButton.addEventListener('click', () => {
                initializeThemePreview('image')
            })

            function initializeScriptPreview(code = null) {
                const displayCode = code || scriptsData.get(lastActiveElement.dataset.communityItemId)
                communityScriptsPreviewCodeEl.textContent = displayCode;
                Prism.highlightElement(communityScriptsPreviewCodeEl)
            }

            // First run
            initializeScriptPreview("# Hover on script name to enable preview")


            function handlePreviewOnHover(e) {
                const selectedItem = e.target.closest('.tab__option');
                const isPointerHoveringOnImportantElement = e.target.closest('[data-keep-preview-on-hover]') != null;
                const currentTab = getCurrentTab();

                pointerType = 'mouse';  
                pointerHovering = isPointerHoveringOnImportantElement;

                // add arrow
                if(selectedItem && selectedItem !== lastActiveElement && lastActiveElement != undefined) {
                    lastActiveElement.dataset.communityCurrentlyPreviewing = false;
                }

                // not hovering on any tab
                if(!selectedItem || lastActiveElement === selectedItem) return;
                lastActiveElement = selectedItem;

                selectedItem.dataset.communityCurrentlyPreviewing = (currentTab === 'community-languages') ? false : true;
                
                if(currentTab === 'community-themes') initializeThemePreview()
                else if (currentTab === 'community-scripts') initializeScriptPreview()
            } 

            const throttleHandlePreviewOnHover = throttle((e)=>{handlePreviewOnHover(e)}, 50)

            communityContentEl.addEventListener('pointermove', throttleHandlePreviewOnHover)
            allCommunitySelectEls.forEach(el=>el.addEventListener('pointermove', throttleHandlePreviewOnHover))

            communityThemesList.addEventListener('keydown', (e) => {
                if(!e.target.matches('.tab__option--themes') || e.key !== 'Enter') return;

                pointerType = 'keyboard';
                lastActiveElement = e.target;
                initializeThemePreview()
            })

            // searchbar
            const CLASS_SHOW = 'show',
                CLASS_HIDDEN = 'hidden';

            function communitySearch(searchQuery, tab) {
                const tabEl = document.querySelector(`#community-content .tab#community-${tab}`);
                const allListItemsToSearchIn = Array.from(tabEl.querySelectorAll(`[data-community-hover-preview="${tab}"] li`));
                const noResultsEl = tabEl.querySelector('.no-results-info');

                communityThemesList.dataset.communityThemesPreviewShow = previewMode;

                let hiddenElsCount = 0;

                allListItemsToSearchIn.forEach((el, i, arr) => {
                    const loopEnd = i === arr.length - 1 // when loop is on the last element

                    let elementMatchesSearchQuery;

                    // search query
                    elementMatchesSearchQuery = Array.from(el.querySelectorAll('[data-community-search-content]'))
                        .reduce((previousValue, currentValue) => {
                            return previousValue + ' ' + currentValue.textContent.toLowerCase()
                        }, '')
                        .includes(searchQuery);
                    

                    // extra options for themes tab
                    if(tab === 'themes') {
                        if(previewMode !== 'both' &&  el.dataset.communityThemesPreviewAvailable !== 'both' && previewMode !== el.dataset.communityThemesPreviewAvailable) {
                            elementMatchesSearchQuery = false;
                        }
                    }

                    // disabling/enabling
                    if(!elementMatchesSearchQuery) {
                        el.classList.add(CLASS_HIDDEN)
                        hiddenElsCount++;
                    }
                    else el.classList.remove(CLASS_HIDDEN);

                    if(!loopEnd) return;

                    if(hiddenElsCount === arr.length) {
                        if(!noResultsEl.classList.contains(CLASS_SHOW)) noResultsEl.classList.add(CLASS_SHOW);
                    } else {
                        noResultsEl.classList.remove(CLASS_SHOW);
                    }
                });

                // reset last active element and hide preview
                lastActiveElement = null;
            }

            // Switch small previews automatically (fade)
            function animateCommunityThemePreviews() {
                if(previewMode !== 'both') return;
                const data = communityThemesList.dataset.communityThemesPreviewShow;

                if(data === 'image' || data === 'both') communityThemesList.dataset.communityThemesPreviewShow = 'embed'
                else if (data === 'embed') communityThemesList.dataset.communityThemesPreviewShow = 'image';
            }
            setInterval(animateCommunityThemePreviews, 5000)

            function getSearchQuery(tab) {
                return allCommunitySearchInputs.find(el => el.id === `community-${tab}-search`).value.trim().toLowerCase();
            }

            function handleCommunitySearch(e) {
                const tab = e.target.dataset.communitySearch;
                communitySearch(getSearchQuery(tab), tab);
            }
            allCommunitySearchInputs.forEach(el => el.addEventListener('input', handleCommunitySearch))

            function handleResetCommunitySearchInput(e) {
                const btnClicked = e.target.closest('button')
                if(!btnClicked) return;

                const correspondingInput = btnClicked.parentElement.querySelector('input');
                correspondingInput.value = null;
                correspondingInput.dispatchEvent(new Event('input'))
            }
            allCommunitySearchInputResetButtons.forEach(el => el.addEventListener('click', handleResetCommunitySearchInput));

            // Preview settings
            function handleCommunityPreviewSettingUpdate(e) {
                const parentBoxEl = e.target.closest('[data-community-themes-preview-setting]');
                if(!parentBoxEl) return;
                
                // disable all other inputs
                if(parentBoxEl.dataset.communityThemesPreviewSetting === 'disable-1') {
                    allCommunityThemesPreviewSetEls.filter(el=>el.dataset.communityThemesPreviewSetting === 'disable-1').forEach(el => {
                        if(el !== parentBoxEl) el.querySelector('input').checked = false;
                    })
                }

                let newPreviewMode = 'both';

                if(disableImageModeCb.checked) newPreviewMode = 'embed';
                else if (disableEmbedModeCb.checked) newPreviewMode = 'image';

                disablePreviewAutoHide = (disablePreviewAutoHideCb.checked) ? true : false;

                previewMode = newPreviewMode;

                communitySearch(getSearchQuery('themes'), 'themes');
            }
            allCommunityThemesPreviewSetEls.forEach(el => el.addEventListener('change', handleCommunityPreviewSettingUpdate))

            function handleCustomCbClick(e) {
                // Handle clikc on the custom checkbox
                if(!e.target.matches('.custom-cb-input__dot')) return;
                const input = e.target.parentElement.querySelector('input')
                input.checked = !input.checked;
                input.parentElement.dispatchEvent(new Event('change'));
            }
            allCustomCbInputs.forEach(el => el.addEventListener('click', handleCustomCbClick))

            function sanitizeHtml(text) {
                const element = document.createElement('div');
                element.innerText = text;
                const sanitizedText = element.innerHTML;
                element.remove();
                return sanitizedText;
            }

            async function dispatchCommunityUserEvent(buttonEl, mode, id) {
                const buttonSpanEl = buttonEl.querySelector('span');

                buttonEl.classList.add('loading');
                buttonEl.disabled = true;

                const success = await onCommunityAction(mode, id);

                if(success) {
                    buttonEl.classList.remove('loading');
                    if(mode === 'themes') {
                        buttonSpanEl.textContent = 'Applied';
                        buttonEl.classList.add('button--success');
                        setTimeout(() => {
                            buttonEl.disabled = false;
                            buttonSpanEl.textContent = 'Apply'
                            buttonEl.classList.remove('button--success');
                        }, 3000);
                    }
                    if(mode === 'scripts') {
                        const dontShowAgain = dialogData["SCRIPT_ADDED"].dontShowAgain;
                        buttonSpanEl.textContent = 'Installed';
                        if(!dontShowAgain) {
                            openDialog('SCRIPT_ADDED');
                        }
                    }
                }
            }

            // apply/install button
            function handleUserActionButtonClick(e) {
                const el = e.target.closest('[data-community-user-action-event]');
                if (!el) return;
                const mode = el.dataset.communityUserActionEvent;
                if(mode === undefined) return;

                const id = el.closest('[data-community-item-id]').dataset.communityItemId;

                dispatchCommunityUserEvent(el, mode, id);
            }

            allSelectEls.forEach(el=>el.addEventListener('click', handleUserActionButtonClick))

            const removeAllCommunityItems = (selectUlEl) => Array.from(selectUlEl.querySelectorAll('li')).forEach(el=>el.remove())

            function updateCommunityThemes(themes = []) {
                if(themes.length === 0) return;
                removeAllCommunityItems(communityThemesList)

                communityThemesList.dataset.communityListState = 'ready';

                themes.forEach(theme => {
                    let themePreviewAvailable = 'both';

                    // Sanitize data
                    for(const themeProp in theme) {
                        theme[themeProp] = sanitizeHtml(theme[themeProp])
                    }

                    if(!theme.imageModePreview) themePreviewAvailable = 'embed';
                    if(!theme.embedModePreview) themePreviewAvailable = 'image';

                    const html =
                    `
                    <li data-community-currently-previewing="false" tabindex="0" data-community-item-id="${theme.name}" data-community-themes-preview-available="${themePreviewAvailable}" class="tab__option tab__option--themes">
                        <div class="theme-input-preview" aria-hidden="true">
                            ${(theme.imageModePreview) ? `<img class="image" src="${theme.imageModePreview}" alt="Theme preview">` : '' }
                            ${(theme.embedModePreview) ? `<img class="embed" src="${theme.embedModePreview}" alt="Theme preview">` : '' }
                        </div>
                        <div class="option-details">
                            <p data-community-search-content>${theme.name}</p>
                            <p data-community-search-content>By: ${theme.author}</p>
                        </div>
                        <button type="button" data-community-user-action-event="themes" class="btn-community option-button button--cta"><span>Apply</span></button>
                    </li>
                    `;

                    communityThemesList.insertAdjacentHTML('beforeend', html)
                })

            }

            function updateCommunityScripts(scripts = []) {
                if(scripts.length === 0) return;

                removeAllCommunityItems(communityScriptsList)
                
                communityScriptsList.dataset.communityListState = 'ready';

                scripts.forEach(script => {
                    // sanitize
                    for(const scriptProp in script) {
                        if(scriptProp === 'installed' || scriptProp === 'code') continue;
                        script[scriptProp] = sanitizeHtml(script[scriptProp])
                    }

                    scriptsData.set(script.name, script.code);

                    const html = `
                    <li data-community-currently-previewing="false" tabindex="0" data-community-item-id="${script.name}" class="tab__option tab__option--scripts">
                        <div class="option-details">
                            <p data-community-search-content>${script.name}</p>
                            <p data-community-search-content>${script.description}</p>
                            <p data-community-search-content>By: ${script.author}</p>
                        </div>
                        <button data-community-user-action-event="scripts" type="button" ${(script.installed) ? 'disabled' : ''} class="btn-community option-button button--cta"><span>${(script.installed) ? 'Installed' : 'Install'}<span></button>
                    </li>
                    `

                    communityScriptsList.insertAdjacentHTML('beforeend', html)
                })                
            }


            function updateCommunityLanguages(languages = []) {
                if(languages.length === 0) return;

                removeAllCommunityItems(communityLanguagesList)

                communityLanguagesList.dataset.communityListState = 'ready';

                languages.forEach(language => {
                    // sanitize
                    for(const languageProp in language) {
                        if(languageProp === 'applied') continue;
                        language[languageProp] = sanitizeHtml(language[languageProp])
                    }

                    const html = `
                    <li data-community-item-id="${language.name}" class="tab__option tab__option--languages">
                        <div class="theme-input-preview" aria-hidden="true">
                            <img src="${language.flagUrl}" alt="${language.name} flag">
                        </div>
                        <div class="option-details">
                            <p data-community-search-content>${language.name}</p>
                            <p data-community-search-content>By: ${language.author}</p>
                        </div>
                        <button data-community-user-action-event="languages" type="button" ${(language.applied) ? 'disabled' : ''} class="btn-community option-button button--cta"><span>${(language.applied) ? 'Applied' : 'Apply'}</span></button>
                    </li>
                    `
                    communityLanguagesList.insertAdjacentHTML('beforeend', html)
                })                
            }
            
        </script>

        <script>
            // ---- Community ------

            // Update themes on the UI
            // at least one image link is required, imageModePreview, embedModePreview or both
            window.addEventListener('pywebviewready', function() {
                pywebview.api.getCommunityThemes().then(
                    (themes_data) => {
                        updateCommunityThemes(themes_data);
                    }
                )
                pywebview.api.getCommunityScripts().then(
                    (scripts_data) => {
                        updateCommunityScripts(scripts_data);
                    }
                )
                pywebview.api.getCommunityLangs().then(
                    (lang_data) => {
                        updateCommunityLanguages(lang_data);
                    }
                )
            });


            // Community action (user clicked button) handler
            async function onCommunityAction(mode, id) {
                if(mode === 'themes') {
                    // User clicked the "apply" (theme) button
                    // id = theme name
                    // handle py code here
                    pywebview.api.applyTheme(id).then(
                        (response) => {
                            // theme applied response
                            console.log(response);
                            if (response.includes('Applied')) {
                                return true;
                            }
                        }
                    )
                }

                if(mode === 'scripts') {
                    // User clicked the "install" (script) button
                    // id = script name

                    // handle py code here
                    pywebview.api.installScript(id).then(
                        (response) => {
                            // script installed response
                            console.log(response);
                            if (response.includes('Installed')) {
                                return true;
                            }
                        }
                    )
                }
                
                if(mode === 'languages') {
                    // User clicked the "apply" (language) button
                    // id = language name

                    // handle py code here
                    pywebview.api.applyLanguage(id).then(
                        (response) => {
                            // language applied response
                            console.log(response);
                            if (response.includes('Applied')) {
                                return true;
                            }
                        }
                    )
                }
                return true;
            }

             // ---- Community END ------
        </script>

        <script>
        "use strict";

        // ==============================================================
        // ======================== DIALOG ==============================

        const dialog = document.getElementById('dialog')
        const dialogWrapper = document.getElementById('dialog-wrapper')
        const dialogOverlay = document.getElementById('dialog-overlay')
        const dialogContent = document.getElementById('dialog-content')
        const dialogTemplates = [...document.querySelectorAll('[data-template]')]

        // vars and fns for all types of dialogs
        let dialogOnScreen = false,
            isDialogClickable = false, // dialog is not clickable when its in a entering/leaving state
            registerAction = undefined, // action that was registered when dialog was not clickable
            closeAvailable = true, // ability to close the dialog
            customContentListener = undefined, // custom event listener (click) on dialogContent element 
            alertActive = false,
            nextAlerts = [], // alerts to be displayed if there is already an alert on the dialog
            switchAccsFailedEventAttached = false,
            newAccSuccessEventAttached = false,
            newAccErrorEventAttached = false,
            deleteAccSuccessEventAttached = false,
            deleteAccFailedEventAttached = false;

        const sleep = (ms) => new Promise(resolve => setTimeout(resolve, ms))

        const getDialogContentHeight = () => dialogContent.offsetHeight;
        
        const insertNewDialogContentAndAnimate = (
            elements = [{htmlElement: HTMLElement, newContent: ''}],
            elementsToRemove = [],
            previousHeight = undefined // will be used to calculate the new height, only use if needed
        ) => {
            const saveContentHeight = previousHeight || getDialogContentHeight();
            const saveDialogHeight = +dialog.style.height.split('px')[0]

            if(elementsToRemove.length !== 0) {
            elementsToRemove.forEach(el => el.remove())
            }

            if(elements.length > 0) {
            elements.forEach(el => {
                el.htmlElement.textContent = el.newContent 
            })
            }

            const newContentHeight = dialogContent.offsetHeight;
            const contentHeightDifference = saveContentHeight - newContentHeight; 

            dialog.style.height = saveDialogHeight - contentHeightDifference + 'px';
        }

        const dialogData = {
            "SWITCH_ACC": {
                onLoad: () => {},
                accountsData: [],
                selectedAccount: '',
                isDeleting: false // Account is currently deleting
            },
            "ADD_NEW_ACC": {
                onLoad: () => {},
                tokenFromClipboard: ''
            },
            "SCRIPT_ADDED": {
                onLoad: () => {},
                dontShowAgain: false,
            }
        }

        // === vars and fns for pre-defined dialogs

        dialogData["SWITCH_ACC"].onLoad = async () => {
            const {accountsData} = dialogData["SWITCH_ACC"];
            let dropdownOpened = false;
            dialogData["SWITCH_ACC"].isDeleting = false; 

            const headingText = dialog.querySelector('.dialog__heading');
            const contentText = dialog.querySelector('.dialog__text');
            const dropdownBox = document.getElementById('dialog-switch-acc-accounts-dropdown');
            const selectAccBtn = document.getElementById('dialog-switch-acc-btn-select-acc');
            const cancelBtn = document.getElementById('dialog-switch-acc-btn-cancel');
            const buttonsBox = dialog.querySelector('.dialog__button-box');
            const closeBtn = dialog.querySelector('.dialog__close-button-wrapper');
            const getDeleteBtn = () => document.getElementById('dialog-switch-acc-btn-delete');

            const generateDropdownHtml = () => {
            const html = accountsData.map(accData => {
                return `
                <button data-select-account="${accData.profileName}" class="accounts-dropdown__button">
                <span class="discord-name">${accData.profileName}</span>
                <span class="discord-token">${accData.discordToken}</span>
                </button>
                `;
            }).join("")

            return html;
            }
            
            const dropdownHtml = generateDropdownHtml();
            dropdownBox.insertAdjacentHTML('beforeend', dropdownHtml);
            // enable delete button if theres only 1 acc
            if (accountsData.length === 1) {
            getDeleteBtn().disabled = false;
            }


            // this goes back to the initial state, after clicking the "back" button
            const goBackToFirstStep = () => {
            const template = document.querySelector('template[data-template="SWITCH_ACC"]');

            const originalContentText = template.content.querySelector('.dialog__text').textContent;

            const originalSelectAccText = template.content.querySelector('#dialog-switch-acc-btn-select-acc').textContent;

            const originalCancelBtnText = template.content.querySelector('#dialog-switch-acc-btn-cancel').textContent;

            insertNewDialogContentAndAnimate([
                {
                htmlElement: contentText,
                newContent: originalContentText
                },
                {
                htmlElement: selectAccBtn,
                newContent: originalSelectAccText
                },
                {
                htmlElement: cancelBtn,
                newContent: originalCancelBtnText
                }
            ])
            
            // disable delete button
            getDeleteBtn().disabled = true;
            selectAccBtn.dataset.state = 'SELECT';
            cancelBtn.setAttribute('data-onclick-close-dialog', '')
            }

            // this is a step after user checks the account to switch, and asks user to confirm
            const renderConfirmStep = () => {
            const {selectedAccount} = dialogData["SWITCH_ACC"];

            const elementsToChange = [
                {
                htmlElement: contentText,
                newContent: `Are you sure you want to switch to ${selectedAccount}? Nighty application has to be reloaded in order to do this.`
                },
                {
                htmlElement: selectAccBtn,
                newContent: 'Switch now!'
                },
            ]

            if(accountsData.length > 1) {
                elementsToChange.push({
                htmlElement: cancelBtn,
                newContent: 'Back'
                })
                cancelBtn.removeAttribute('data-onclick-close-dialog')
            }

            // enable delete button
            getDeleteBtn().disabled = false;

            insertNewDialogContentAndAnimate(elementsToChange)
            selectAccBtn.dataset.state = 'CONFIRM';
            }

            if(accountsData.length === 1) {
            dialogData['SWITCH_ACC'].selectedAccount = accountsData[0].profileName;
            await sleep(100)
            renderConfirmStep();
            }

            // this is a step when user confirmed account switch, and animation is starting
            const renderInProgressStep = async () => {
            const {selectedAccount} = dialogData["SWITCH_ACC"];
            dialogWrapper.classList.add('switch-acc-in-progress');

            await sleep(200)

            insertNewDialogContentAndAnimate([
                {
                htmlElement: headingText,
                newContent: `Switching accounts...`
                }
            ],
            [contentText, buttonsBox, closeBtn]
            )

            closeAvailable = false;

            window.dispatchEvent(new CustomEvent('switchaccs', {detail: selectedAccount}))
            }

            const dispatchDeleteAccount = () => {
            const {selectedAccount} = dialogData["SWITCH_ACC"];
            window.dispatchEvent(new CustomEvent('deleteacc', {detail: selectedAccount}));
            dialogData["SWITCH_ACC"].isDeleting = true;
            }

            const openDropdown = () => {
            dropdownBox.classList.remove('hidden');
            dropdownOpened = true;
            }

            const closeDropdown = () => {
            dropdownBox.classList.add('hidden');
            dropdownOpened = false;
            }

            customContentListener = (e) => {
            if(dialogData["SWITCH_ACC"].isDeleting) return;

            // "Select an account" button
            const selectBtn = e.target.closest('#dialog-switch-acc-btn-select-acc');
            if(selectBtn && selectBtn.dataset.state === 'SELECT') {

                openDropdown()
                return;
            }

            // "Switch!" button
            if(selectBtn && selectBtn.dataset.state === 'CONFIRM') {

                renderInProgressStep()
                return;
            }

            // "Back" button
            const backBtn = e.target.closest('#dialog-switch-acc-btn-cancel')
            if(backBtn) {
                e.stopPropagation()

                if(backBtn.hasAttribute('data-onclick-close-dialog')) {
                closeDialog()
                return;
                }

                goBackToFirstStep()
                return;
            }

            // "Delete button"
            const deleteButton = e.target.closest('#dialog-switch-acc-btn-delete')
            if(deleteButton && getDeleteBtn().disabled === false) {
                getDeleteBtn().classList.add('btn-loading');
                getDeleteBtn().disabled = true;
                closeAvailable = false;
                dispatchDeleteAccount();
                return;
            }

            const selectedAccEL = e.target.closest('[data-select-account]');

            // Click outside the dropdown
            if(!selectedAccEL && dropdownOpened) {
                closeDropdown();
                return;
            }

            // Select an account from the dropdown
            if(selectedAccEL) {
                dialogData["SWITCH_ACC"].selectedAccount = selectedAccEL.dataset.selectAccount;
        
                closeDropdown();
                renderConfirmStep();
                return;
            }
            }

            // handle switch accs failed event


            const handleSwitchAccsFailed = (e) => {
            if(!dialogOnScreen) return;

            let intervalID, i = 9;
            
            let {title, message} = e.detail;

            const updateHeadingText = () => {
                if(i === 1) clearInterval(intervalID)

                insertNewDialogContentAndAnimate([
                {
                    htmlElement: document.querySelector('.dialog .dialog__heading'),
                    newContent: `Account switching failed. Closing in ${i} seconds...`,
                }
                ], [])

                i--;
            }

            intervalID = setInterval(updateHeadingText, 1000);

            displayAlert({ type: 'ERROR', title, message })
            setTimeout(closeDialog.bind(null, true), 10000)
            }

            const handleDeleteAccSuccess = async (e) => {
            dialogData["SWITCH_ACC"].isDeleting = false;
            if(dialogData['SWITCH_ACC'].accountsData.length === 0) {
                closeAvailable = true;
                document.getElementById('dialog-switch-acc-btn-select-acc').disabled = true;
                const deleteBtn = getDeleteBtn();
                deleteBtn.classList.remove('btn-loading');
                deleteBtn.disabled = true;
                const backBtnReselect = document.getElementById('dialog-switch-acc-btn-cancel');
                insertNewDialogContentAndAnimate([
                {
                    htmlElement: document.querySelector('.dialog .dialog__text'),
                    newContent: `No accounts available.`,
                },
                {
                    htmlElement: backBtnReselect,
                    newContent: `Close`,
                },
                ], [])
                backBtnReselect.setAttribute('data-onclick-close-dialog', '');
                displayAlert({
                type: 'SUCCESS',
                title: 'Profile deleted',
                message: 'Profile has been deleted. You can now close this window.'
                })
            return;
            }
            await closeDialog(true);
            await openDialog('SWITCH_ACC');
            displayAlert({
                type: 'SUCCESS',
                title: 'Profile deleted',
                message: 'Profile has been deleted. You can now switch to another account or close this window.'
            })
            } 
            const handleDeleteAccFailed = (e) => {
            dialogData["SWITCH_ACC"].isDeleting = false;
            const errorMessage = e.detail || 'Unspecified error.';
            displayAlert({
                type: 'ERROR',
                title: 'An error occured',
                message: errorMessage
            })
            closeAvailable = true;
            getDeleteBtn().classList.remove('btn-loading');
            getDeleteBtn().disabled = false;
            }


            if(!switchAccsFailedEventAttached) {
            window.addEventListener('switchaccsfailed', handleSwitchAccsFailed)
            switchAccsFailedEventAttached = true;
            }

            if(!deleteAccSuccessEventAttached) {
            window.addEventListener('deleteaccsuccess', handleDeleteAccSuccess)
            deleteAccSuccessEventAttached = true;
            }
            
            if(!deleteAccFailedEventAttached) {
            window.addEventListener('deleteaccfailed', handleDeleteAccFailed)
            deleteAccFailedEventAttached = true;
            }


            // all click events
            dialogContent.addEventListener('click', customContentListener)
        }



        dialogData["ADD_NEW_ACC"].onLoad = async () => {
            let tokenPasted = false,
            waitingForAddedEvent = false;

            const profileNameInputWrapper = document.getElementById('profile-name-input-wrapper');
            const tokenInputWrapper = document.getElementById('token-input-wrapper');
            const passwordInputWrapper = document.getElementById('password-input-wrapper');

            const profileNameInput = document.getElementById('new-account-profile');
            const discordTokenInput = document.getElementById('new-account-token');
            const discordPasswordInput = document.getElementById('new-account-password');

            const profileErrorEl = profileNameInputWrapper.querySelector('.input-error');
            const tokenErrorEl = tokenInputWrapper.querySelector('.input-error');
            const passwordErrorEl = passwordInputWrapper.querySelector('.input-error');

            const addButton = document.getElementById('new-acc-add');
            const newProfileForm = document.querySelector('#dialog #new-profile-form');
            const tokenInputVisibilityBtn = document.querySelector('#token-input-wrapper .input-visible-button');
            const passwordInputVisibilityBtn = document.querySelector('#password-input-wrapper .input-visible-button');
            const btnBox = document.querySelector('.dialog .dialog__button-box');

            const validateProfileName = (profileName) => {
            const regex = new RegExp(/[a-zA-Z0-9]/);
            const MIN_LENGTH = 2;
            const MAX_LENGTH = 100;

            let isValid = true, extraError = null;
            profileName = profileName.trim();

            if(regex.test(profileName) === false) {
                isValid = false;
                extraError = 'The inputted profile name contains invalid characters.'
            }

            if(profileName.includes(" ")) {
                isValid = false;
                extraError = 'Invalid character (space)';
            }
            
            if(profileName.length < MIN_LENGTH) {
                isValid = false;
                extraError = `Profile name too short (must have at least ${MIN_LENGTH} characters)`;  
            }

            if(profileName.length > MAX_LENGTH) {
                isValid = false;
                extraError = `Max characters length (${MAX_LENGTH}) exceeded.`;  
            }
            
            const profileAlreadyExists = dialogData["SWITCH_ACC"].accountsData.find(acc => acc.profileName.toLowerCase() === profileName.toLowerCase())
            
            if(profileAlreadyExists) {
                isValid = false;
                extraError = `A profile with this name already exists.`;  
            }

            return [profileName, isValid, extraError]
            }

            const validateDiscordToken = (token) => {
            let isValid = true, extraError = null;
            const regex = new RegExp(/[a-zA-Z0-9.-]/);
            const MIN_LENGTH = 40;
            const MAX_LENGTH = 100;

            token = token.trim();

            if(regex.test(token) === false) {
                isValid = false;
                extraError = 'The inputted token contains invalid characters.'
            }

            if(token.length < MIN_LENGTH) {
                isValid = false;
                extraError = 'Token too short.'
            }

            if(token.length > MAX_LENGTH) {
                isValid = false;
                extraError = 'Token too long.'
            }

            if(token.includes(" ")) {
                isValid = false;
                extraError = 'Invalid character (space)';
            } 

            return [token, isValid, extraError]
            }

            const validateDiscordPassword = (password) => {
            let isValid = true, extraError = null;
            const MIN_LENGTH = 8;

            password = password.trim();

            if(password.length < MIN_LENGTH) {
                isValid = false;
                extraError = 'Password too short.'
            }

            return [password, isValid, extraError]
            }

            const addErrorState = (element) => element.classList.add('error');
            const removeErrorState = (element) => element.classList.remove('error');

            const submitHandler = (e) => {
            const [profileName, profileNameIsValid, profileNameError] = validateProfileName(profileNameInput.value);
            const [token, tokenIsValid, tokenError] = validateDiscordToken(discordTokenInput.value);
            const [password, passwordIsValid, passwordError] = validateDiscordPassword(discordPasswordInput.value);

            e.target.blur();

            const saveDialogContentHeight = getDialogContentHeight();

            if(profileNameIsValid && tokenIsValid && passwordIsValid) {
                // adding profile profile
                window.dispatchEvent(new CustomEvent('addnewacc', {detail: {
                profileName,
                token,
                password
                }}))

                insertNewDialogContentAndAnimate([
                {
                    htmlElement: document.querySelector('.dialog .dialog__heading'),
                    newContent: 'Adding profile'
                },
                {
                    htmlElement: document.querySelector('.dialog .dialog__text'),
                    newContent: 'Please wait...'
                },
                ], [
                newProfileForm
                ]);

                const saveDialogContentHeight2 = getDialogContentHeight();
                btnBox.style.display = 'none';
                insertNewDialogContentAndAnimate([], [], saveDialogContentHeight2);

                dialogWrapper.classList.add('add-acc-in-progress');
                waitingForAddedEvent = true;

                return;
            }

            if(!profileNameIsValid) {
                addErrorState(profileNameInputWrapper);
                profileErrorEl.textContent = profileNameError || 'Something went wrong.'
            }

            if(!tokenIsValid) {
                addErrorState(tokenInputWrapper);
                tokenErrorEl.textContent = tokenError || 'Something went wrong.'
            }

            if(!passwordIsValid) {
                addErrorState(passwordInputWrapper);
                passwordErrorEl.textContent = passwordError || 'Something went wrong.'
            }

            insertNewDialogContentAndAnimate([], [], saveDialogContentHeight)
            }

            const inputHandler = (e) => {
            const saveDialogContentHeight = getDialogContentHeight();
            removeErrorState(e.target.closest('.input-wrapper'))
            insertNewDialogContentAndAnimate([], [], saveDialogContentHeight)
            }

            const switchInputVisibilityState = (e) => {
            const inputEl = e.target.closest('.input-wrapper').querySelector('.dialog__input');
            const clickedButton = e.target.closest('.input-visible-button');
            const isInputVisible = clickedButton.dataset.visible === 'true';

            clickedButton.dataset.visible = !isInputVisible;
            inputEl.type = (!isInputVisible) ? 'text' : 'password';
            }

            // try to paste token
            const handleTokenInputFocus = async (e) => {
            if(e.target.value.trim() !== '' || tokenPasted) return;
            
            const textFromClipobard = pywebview.api.getClipboardText();
            const [clipboardToken, ctokenIsValid] = validateDiscordToken(textFromClipobard);

            if(!ctokenIsValid) return;
            discordTokenInput.value = clipboardToken;
            removeErrorState(e.target.closest('.input-wrapper'));
            tokenPasted = true;
            displayAlert({
                type: 'SUCCESS',
                title: 'Pasted from clibpoard',
                message: 'Token pasted from clipboard successfully.'
            })
            }

            newProfileForm.addEventListener('input', inputHandler);
            newProfileForm.addEventListener('submit', (e) => {
            e.preventDefault();
            submitHandler(e);
            });
            addButton.addEventListener('click', submitHandler);
            tokenInputVisibilityBtn.addEventListener('click', switchInputVisibilityState);
            passwordInputVisibilityBtn.addEventListener('click', switchInputVisibilityState);
            // discordTokenInput.addEventListener('focus', handleTokenInputFocus)

            const handleAccAddedSuccess = async () => {
            if(!dialogOnScreen) return;

            insertNewDialogContentAndAnimate([
                {
                htmlElement: document.querySelector('.dialog .dialog__heading'),
                newContent: 'Profile added'
                },
                {
                htmlElement: document.querySelector('.dialog .dialog__text'),
                newContent: 'Choose what to do now.'
                },
            ]);

            dialogWrapper.classList.remove('add-acc-in-progress');

            displayAlert({
                type: 'SUCCESS',
                title: 'Profile added',
                message: 'Profile has been added succesfully. You can now switch to the new account.'
            })

            // add buttons back
            const btnBox2 = document.querySelector('.dialog .dialog__button-box');

            const saveDialogContentHeight3 = getDialogContentHeight();
            const oldAddBtn = btnBox2.querySelector('#new-acc-add');
            const openSwitchAccsDialogBtn = oldAddBtn.cloneNode(true);
            btnBox2.querySelector('.account-button').remove();

            openSwitchAccsDialogBtn.id = 'open-switch-accs-dialog';
            openSwitchAccsDialogBtn.textContent = 'Switch account';
            btnBox2.style.display = 'flex';

            btnBox2.append(openSwitchAccsDialogBtn);

            insertNewDialogContentAndAnimate([], [], saveDialogContentHeight3);

            openSwitchAccsDialogBtn.addEventListener('click', async () => {
                await closeDialog();
                openDialog('SWITCH_ACC');
            })

            }

            const handleAccAddedError = (e) => {
            if(!dialogOnScreen) return;

            const errorMessage = e.detail;

            insertNewDialogContentAndAnimate([
                {
                htmlElement: document.querySelector('.dialog .dialog__heading'),
                newContent: 'Unable to add account'
                },
                {
                htmlElement: document.querySelector('.dialog .dialog__text'),
                newContent: 'Please close this window and try again later.'
                },
            ]);

            dialogWrapper.classList.remove('add-acc-in-progress');

            displayAlert({
                type: 'ERROR',
                title: 'Failed to add profile',
                message: errorMessage
            })
            }

            // global listeners

            if(!newAccSuccessEventAttached) {
            window.addEventListener('addnewaccsuccess', handleAccAddedSuccess)
            newAccSuccessEventAttached = true;
            }

            if(!newAccErrorEventAttached) {
            window.addEventListener('addnewaccerror', handleAccAddedError)
            newAccErrorEventAttached = true;
            }
        }
        // ===

        dialogData["SCRIPT_ADDED"].onLoad = async () => {
            const restartBtn = document.getElementById('dialog-script-added-restart-nighty');
            const dontShowAgainBtn = document.getElementById('script-added-dont-show-again');
            function handleRestartBtnClick() {
                closeAvailable = false;
                insertNewDialogContentAndAnimate([
                {
                    htmlElement: document.querySelector('.dialog .dialog__heading'),
                    newContent: 'Restarting...'
                },
                {
                    htmlElement: document.querySelector('.dialog .dialog__text'),
                    newContent: 'Please wait...'
                },
                ], [
                document.querySelector('.dialog .dialog__button-box'),
                document.querySelector('.dialog .dialog__dont-show'),
                document.querySelector('.dialog .dialog__close-button-wrapper'),
                document.querySelector('.dialog .dialog__icon'),
                ]);
                pywebview.api.restartBot();
            }
            restartBtn.addEventListener('click', handleRestartBtnClick)
            function handleDontShowAgainBtnClick() {
                dialogData["SCRIPT_ADDED"].dontShowAgain = true;
                closeDialog();
            }
            dontShowAgainBtn.addEventListener('click', handleDontShowAgainBtnClick)
        }


        const executeRegisteredAction = () => {
            if(!registerAction) return;
            registerAction()
            registerAction = undefined;
        }

        const resetDialog = () => {
            dialogWrapper.classList = 'dialog-wrapper'
            dialogWrapper.style.visibility = 'hidden';
            dialogWrapper.style.zIndex = '-1'
            dialogContent.innerHTML = '';
            dialog.style.height = null;
            dialog.style.width = null;
            nextAlerts = [];
            alertActive = false;
            dialogOnScreen = false;
            closeAvailable = true;
            registerAction = undefined;
        }

        const insertDialogTemplate = (templateName) => {
            const templateEl = document.querySelector(`template[data-template="${templateName}"]`);
            const clonedEl = templateEl.content.cloneNode(true);

            const appendingFunction = () => dialogContent.prepend(clonedEl);

            return [ clonedEl, appendingFunction ];
        }

        const openDialog = async (dialogType = 'CUSTOM') => {
            dialogOnScreen = true;
            isDialogClickable = false;

            // activate the correct template
            const [_, appendTemplate] = insertDialogTemplate(dialogType);
            appendTemplate();

            // execute starting function
            dialogData[dialogType].onLoad()

            const saveHeight = dialog.offsetHeight;
            dialog.style.maxHeight = '100vh';
            dialog.style.width = dialog.offsetWidth + 50 + 'px';
            dialog.style.height = '0';
            dialogWrapper.style.zIndex = 6;
            dialogWrapper.style.visibility = 'visible';
            dialogWrapper.classList.add('entering')
            await sleep(1);
            dialog.style.height = saveHeight + 'px';

            await new Promise(resolve => dialog.addEventListener('animationend', resolve))

            dialogWrapper.classList.add('active');
            dialogWrapper.classList.remove('entering');
            isDialogClickable = true;

            executeRegisteredAction();

            return true;
        }

        const closeDialog = async (forceClose = false) => {
            if(!closeAvailable && !forceClose) return;

            if(customContentListener) {
            dialogContent.removeEventListener('click', customContentListener);
            }

            isDialogClickable = false;
            dialogWrapper.classList.remove('active');
            dialogWrapper.classList.add('leaving');
            await sleep(1);
            dialog.style.height = '0';

            await new Promise(resolve => dialog.addEventListener('animationend', resolve))
            
            resetDialog();
            dialogOnScreen = false;
            isDialogClickable = true;

            return true;
        }

        const displayAlert = ({
            type = '', // SUCCESS, ERROR
            title = '',
            message = ''
        }, forceDisplay = false) => {
            if(!dialogOnScreen) return;

            if(alertActive && forceDisplay === false) {
            nextAlerts.push({type, title, message})
            return;
            }

            alertActive = true;

            const generateAlertID = Math.random();


            const REMOVE_AFTER_MS = 10000;
            const [alertElFragment, appendAlert] = insertDialogTemplate('DIALOG_ALERT');
            const alertEl = alertElFragment.children[0];

            alertEl.dataset.alertId = generateAlertID;
            alertEl.classList.add(`dialog__alert--${type.toLocaleLowerCase()}`)
            alertEl.querySelector('.dialog__alert-title').textContent = title;
            alertEl.querySelector('.dialog__alert-message').textContent = message;

            const handleAlertClick = (e) => {
            const alertCloseBtn = e.target.closest('.dialog__alert-close')
            if(!alertCloseBtn) return;

            const alertID = document.getElementById('dialog-alert').dataset.alertId;
            hideAlert(alertID, handleAlertClick)
            }

            alertEl.addEventListener('click', handleAlertClick)
            
            setTimeout(hideAlert.bind(null, generateAlertID, handleAlertClick), REMOVE_AFTER_MS)
            
            const saveDialogContentHeight = getDialogContentHeight()

            appendAlert();

            insertNewDialogContentAndAnimate([], [], saveDialogContentHeight)
        }
        
        const hideAlert = (alertID, clickListener = null) => {
            if(!dialogOnScreen) {
            nextAlerts = [];
            return;
            }

            const alertEl = document.getElementById('dialog-alert');  
            if(!alertEl || alertEl.dataset.alertId !== alertID) return;

            insertNewDialogContentAndAnimate([], [alertEl]);

            if(clickListener) {
            alertEl.removeEventListener('click', clickListener);
            }

            if(nextAlerts.length === 0) {
            alertActive = false;
            return;
            }

            // showing dialogs that are waiting to be displayed
            displayAlert(nextAlerts[0], true);
            nextAlerts.shift();
        }

        // setup listeners valid for any type of dialog
        dialogWrapper.addEventListener('click', async (e) => {
            if(!dialogOnScreen) return;

            if(dialogOnScreen && !isDialogClickable) {
            registerAction = closeDialog;
            return;
            }

            if(e.target.closest('[data-onclick-close-dialog]') == null) return;

            await closeDialog()
            executeRegisteredAction();
        })
        
        window.addEventListener('keydown', async (e) => {
            if(!document.hasFocus() || e.key !== 'Escape') return;

            if(!dialogOnScreen) return;

            if(dialogOnScreen && !isDialogClickable) {
            registerAction = closeDialog;
            return;
            }
            
            await closeDialog();
            executeRegisteredAction();
        })

        // listeners end

        // Switch accounts dialog
        function openSwitchAccDialog(e) {
            if(dialogOnScreen) return;
            e.target.blur()

            openDialog('SWITCH_ACC')
        }
        
        function openAddNewAccDialog(e) {
            if(dialogOnScreen) return;
            e.target.blur()
        
            openDialog('ADD_NEW_ACC')
        }

        // Switch accs button
        const switchAccBtn = document.getElementById('switch-account-button')
        switchAccBtn.addEventListener('click', openSwitchAccDialog)
        
        // Add neww acc button
        const addNewAccBtn = document.getElementById('add-new-account-button')
        addNewAccBtn.addEventListener('click', openAddNewAccDialog)


        // ====================== DIALOG END ============================
        // ==============================================================

        const generateGradientInlineStyle = (deg) =>
            `conic-gradient(var(--accent-color-main) ${deg}deg,var(--card-background-color) 0deg)`;

        const userCard = document.querySelector(".card--user");
        const selfbotCard = document.querySelector(".card--selfbot");
        const globalCard = document.querySelector(".card--global");

        // Can be used when Nighty is connecting to user's account and none of the user information is known yet
        function setCardLoading(cardElement, loading = false, ) {
            if (loading) cardElement.classList.add("loading");
            else cardElement.classList.remove("loading");

            // remove graph data
            const allGraphs = [...cardElement.querySelectorAll('.graph-circle')];
            if(allGraphs.length === 0) return;

            allGraphs.forEach(graph => {
            if(loading && graph.style.background !== '') {
                graph.dataset.lastDeg = graph.style.background.split(" ")[1].split("deg")[0];
                graph.style.background = null
            } else if (!loading && graph.dataset.lastDeg !== '') {
                graph.style.background = generateGradientInlineStyle(+graph.dataset.lastDeg);
            }
            })

        }

        /*
        Example usage
        const userCard = document.querySelector(".card--user");
        setCardLoading(userCard, true);
        */

        /////////////////////////////////////////////////////////////////////////

        // Switch user status on the UI
        // online, dnd, idle, offline
        function switchUserStatus(status = "online") {
            const statusesCorrectCase = new Map([
            ["online", "Online"],
            ["dnd", "Do Not Disturb"],
            ["idle", "Idle"],
            ["offline", "Offline"],
            ]);

            const correctStatuses = [...statusesCorrectCase.keys()].join(", ");

            if (!statusesCorrectCase.has(status))
            return console.error(
                `Invalid status provided. Calling "switchUserStatus" function. Status provided: ${status}. Available statses: ${correctStatuses}.`
            );

            const circleStatus = document.getElementById("user-status-circle");
            const textStatus = document.getElementById("user-status-text");

            circleStatus.dataset.status = status;
            textStatus.textContent = statusesCorrectCase.get(status);
        }

        /////////////////////////////////////////////////////////////////////////

        // Updates servers and friends graphs
        /*

        Example usage:

        updateGraphsAndCountAnimations(
        {
            serverCount: INT,
            maxServerCount: INT,
            friendsCount: INT,
            maxFriendsCount INT,
        }, 
        [
            {
            htmlElementToAnimate: HTMLElement,
            count: INT
            },
        ],
        animationEnabled: TRUE/FALSE)

        animationEnabled should only be set to true if the user opens the UI for the first time,
        the animation goes from 0 to the server/friends count number
        */

        function updateGraphsAndCountAnimations(
            { serverCount, maxServerCount, friendsCount, maxFriendsCount },
            extraElementsToAnimate = [],
            animationEnabled = false
        ) {
            const MAX_DEG = 360; // Maximum rotate of the conic gradient, default: 360 (deg)
            const ANIMATION_LENGTH_MS = 800;

            // Calculates the target DEG based on users servers and friends amount
            const serverDegTarget = (MAX_DEG * serverCount) / maxServerCount;
            const friendsDegTarget = (MAX_DEG * friendsCount) / maxFriendsCount;

            // -- helper functions --

            // Function selects html elements that will later get updated
            // returns [graph(div), graphCount(span), tooltip(span)]
            const selectElements = (parentGraphID) => {
            return [
                document.getElementById(parentGraphID),
                document.querySelector(`#${parentGraphID} .graph-circle__count`),
                document.querySelector(`#${parentGraphID} .graph-circle__tooltip strong`),
            ];
            };

            const updateGraphs = () => {
            serversGraphEl.style.background =
                generateGradientInlineStyle(serverDegTarget);
            friendsGraphEl.style.background =
                generateGradientInlineStyle(friendsDegTarget);

            serversGraphCountEl.textContent = serverCount;
            friendsGraphCountEl.textContent = friendsCount;

            extraElementsToAnimate.forEach(el => el.textContent = el.count)
            };
            // -- helper functions end --

            const [serversGraphEl, serversGraphCountEl, serversGraphMaxServersTooltip] =
            selectElements("server-graph");

            const [friendsGraphEl, friendsGraphCountEl, friendsGraphMaxServersTooltip] =
            selectElements("friends-graph");

            // Update grpahs
            let firstTimestamp = null,
            timeLeft = 0,
            endsAtTimestamp = 0;

            function updateGraphsWithAnimation(timestamp) {
            if (firstTimestamp === null) {
                firstTimestamp = timestamp;
            }

            if (endsAtTimestamp === 0) {
                endsAtTimestamp = firstTimestamp + ANIMATION_LENGTH_MS;
            }

            timeLeft = endsAtTimestamp - timestamp;

            // calculate current graph amount based on timestamp
            const correctTimestamp = timestamp - firstTimestamp;

            const calculateCurrentCount = (targetCount) => Math.trunc((targetCount * correctTimestamp) / ANIMATION_LENGTH_MS);

            const serverCountAnim = calculateCurrentCount(serverCount)
            const friendsCountAnim = calculateCurrentCount(friendsCount)

            // calculate current graph DEG based on timestamp
            const currentServersDeg = Math.trunc(
                (serverDegTarget * correctTimestamp) / ANIMATION_LENGTH_MS
            );
            const currentFriendsDeg = Math.trunc(
                (friendsDegTarget * correctTimestamp) / ANIMATION_LENGTH_MS
            );

            // update graph and prevent overdo
            if (currentServersDeg <= serverDegTarget) {
                serversGraphEl.style.background =
                generateGradientInlineStyle(currentServersDeg);

                serversGraphCountEl.textContent = serverCountAnim;
            }

            if (currentFriendsDeg <= friendsDegTarget) {
                friendsGraphEl.style.background =
                generateGradientInlineStyle(currentFriendsDeg);

                friendsGraphCountEl.textContent = friendsCountAnim;
            }

            // Update all extra elements
            if(extraElementsToAnimate.length !== 0) {
                extraElementsToAnimate.forEach(el => {
                const elCountAnim = calculateCurrentCount(el.count)

                // prevent overdo
                if(elCountAnim <= el.count) {
                    el.htmlElementToAnimate.textContent = elCountAnim
                }
                })
            }

            if (timestamp > endsAtTimestamp) {
                // We are done. Update graphs once again to be sure they show the correct number
                updateGraphs();
                return;
            }

            window.requestAnimationFrame(updateGraphsWithAnimation);
            }

            if (animationEnabled) {
            window.requestAnimationFrame(updateGraphsWithAnimation);
            } else {
            updateGraphs();
            }

            // Update MAX server/friends count
            serversGraphMaxServersTooltip.textContent = maxServerCount;
            friendsGraphMaxServersTooltip.textContent = maxFriendsCount;
        }

        /////////////////////////////////////////////////////////////////////////

        // should not be used manually
        function _toggleBadgesBox(enabled = true) {
            const badgesClass = "badges--disabled";
            const badgesParentElement = document.querySelector(".card-user__info");

            if (enabled) {
            badgesParentElement.classList.remove(badgesClass);
            } else {
            badgesParentElement.classList.add(badgesClass);
            }
        }

        /*
        Available badges:
        [
            "hypesquad_balance",
            "hypesquad_bravery",
            "hypesquad_brilliance",
            "nitro_classic",
            "nitro_boost",
            "active_developer",
            "nighty",
            "early_supporter"
        ];

        Example usage: updateUserBadges("active_developer", "nighty");
        */

        function updateUserBadges(...badges) {
            if (badges.length === 0) {
            _toggleBadgesBox(false);
            return;
            }

            _toggleBadgesBox(true);

            const enableBadge = (badge) => (badge.dataset.enabled = true);
            const disableBadge = (badge) => (badge.dataset.enabled = false);

            const allBadgesEls = [...document.querySelectorAll("[data-badge]")];

            allBadgesEls.forEach((badge) => {
            if (badges.includes(badge.dataset.badge)) enableBadge(badge);
            else disableBadge(badge);
            });
        }

        /////////////////////////////////////////////////////////////////////////

        // Sets users PFP. Returns a promise.
        // Promise returns True when image is fully loaded
        // Promise returns False when timeout is reached
        // Default timeout = 7 seconds

        /* 
        Example usage:

        setUserImage('https://example.com/image.jpg').then((imageLoaded) => {
            if(imageLoaded) {
            // something here
            } else {
            // reload the image or show error
            }

            setCardLoading(card, false)
        })

        Handling the promise is optional, can be also used as:
            setUserImage('https://example.com/image.jpg')
        */
        async function setUserImage(imageUrl, timeout = 7000) {
            const imageEl = document.querySelector(".card-user__pfp img");

            const timeoutPromise = new Promise((resolve) => setTimeout(resolve, timeout));

            const imageLoadedPromise = new Promise((resolve) => {
            imageEl.addEventListener("load", resolve);
            });

            const finalPromise = Promise.race([imageLoadedPromise, timeoutPromise]).then(
            (loadResult) => {
                if (loadResult) return true;
                else return false;
            }
            );

            imageEl.src = imageUrl;

            return finalPromise;
        }

        /////////////////////////////////////////////////////////////////////////

        // A couple of smaller functions to update user data
        const updateUserName = (userName, userDiscriminator) => {
            const nameElParent = document.querySelector(
            ".card--user .card-user__username"
            );
            const nameEl = nameElParent.querySelector(".username");
            const discriminatorEl = nameElParent.querySelector(".discriminator");

            nameEl.textContent = userName;
            discriminatorEl.textContent = "#" + userDiscriminator;
        };

        const updatePrefix = (prefix) => {
            document.getElementById('selfbot_prefix').textContent = prefix;
        }
        
        const updateVersion = (version) => {
            document.getElementById('selfbot_version').textContent = version;
        }

        /////////////////////////////////////////////////////////////////////////

        // Initializes all of the card. Updates all the information given and removes loading state.
        function initializeCards({
            userName = "",
            userDiscriminator = "",
            userBadges = [],
            userImageUrl = "",
            userStatus = "",
            userServers = 0,
            maxUserServers = 0,
            userFriends = 0,
            maxUserFriends = 0,
            extraElementsToAnimate = [],
            prefix = '',
            version = '',
        }) {
            setCardLoading(userCard, true);
            setCardLoading(selfbotCard, true);
            setCardLoading(globalCard, true);
            
            updateUserName(userName, userDiscriminator);
            updateUserBadges(...userBadges);
            switchUserStatus(userStatus);
            updatePrefix(prefix)
            updateVersion(version)
            
            setUserImage(userImageUrl).then(() => {
            setCardLoading(userCard, false);
            setCardLoading(selfbotCard, false);
            setCardLoading(globalCard, false);

            updateGraphsAndCountAnimations(
                {
                serverCount: userServers,
                maxServerCount: maxUserServers,
                friendsCount: userFriends,
                maxFriendsCount: maxUserFriends,
                },
                extraElementsToAnimate,
                true
            );
            });
        }

        function show(shown) {
            document.getElementById(shown).style.display = 'block';
            return false;
        }
        function hide(a, b) {
            document.getElementById(a).style.display = 'none';
            document.getElementById(b).style.display = 'none';
            return false;
        }
