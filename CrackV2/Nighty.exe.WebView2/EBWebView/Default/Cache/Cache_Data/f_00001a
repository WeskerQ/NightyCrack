        //         break;
        //       }
        //     }
        // };
        let extras_loaded = false;
        async function updateSBCard() {
            //setCardLoading(userCard, true);
            while (true) {
            await new Promise(resolve => setTimeout(resolve, 2000));
            pywebview.api.getInfoData().then(
                (info_data) => {
                if (info_data) {
                    if (info_data["extraElementsToAnimate"][0]["count"] != 0 && !extras_loaded){
                        const globalLoginsEl = document.getElementById('global_logins_count')
                        const globalUsersEl = document.getElementById('global_users_count')
                        info_data["extraElementsToAnimate"][0]["htmlElementToAnimate"] = globalLoginsEl;
                        info_data["extraElementsToAnimate"][1]["htmlElementToAnimate"] = globalUsersEl;
                        initializeCards(info_data);
                        pywebview.api.getDiscordAccounts().then(
                            (account_data) => {
                                setAvailableAccounts(account_data);
                            }
                        )
                        extras_loaded = true;
                    }
                    }
                });
                if (extras_loaded){
                    break;
                }
            }
        }
        let elements = document.getElementsByClassName("card-user__username");
        for (let i = 0; i < elements.length; i++) {
            elements[i].addEventListener("click", function() {
                pywebview.api.getInfoData().then( (info_data) => {
                    if (info_data) {
                        pywebview.api.discordJump(`discord://discord.com/users/${info_data["userId"]}`);
                    }
                });
            });
        }
        const icon = document.getElementById('ui-titleicon-new');
        icon.classList.add('animation-active');

        icon.addEventListener('pointerenter', () => {
            icon.classList.add('animation-active');
            toggleSidebar();
        })

        icon.addEventListener('pointerleave', () => {
            icon.classList.remove('animation-active');
        })

        async function setupLang() {
            pywebview.api.NightyTranslate().then( (languagesData) => {
                const allLangElements = [...document.querySelectorAll("[data-lang]")];

                console.log(languagesData);
                console.log(...allLangElements);

                /*function getUserLocale() {
                if (navigator.languages != undefined)
                    return navigator.languages[0].split("-")[0];

                return navigator.language.split("-")[0];
                }
                
                const userLocale = getUserLocale();*/

                for (const el of allLangElements) {
                    const pointer = el.dataset.lang.split(".");
                    const newLangContent = languagesData["UI"]?.[pointer[0]]?.[pointer[1]]?.[pointer[2]]?.[pointer[3]];

                    if (newLangContent === undefined) continue;

                    el.textContent = newLangContent;
                }
            });
        }

        
        window.addEventListener('pywebviewready', function() {
            updateConsole();
            updateConsoleContent();
            //updateProfileCard();
            updateUptime();
            updateInfo();
            updateSBCard();
            updateAvatar();
            updateUserInfo();
            updateGraphsCount();
            setupLang();
        });


        
