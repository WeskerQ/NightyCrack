        async function updateConsoleContent() {
            while (true) {
                await new Promise(resolve => setTimeout(resolve, 1500));
                pywebview.api.getConsoleState().then(
                    (result) => {
                        let consoleUpdate = document.querySelector(".console-output")
                        consoleUpdate.innerHTML = result;
                        consoleUpdate.scrollTop = consoleUpdate.scrollHeight;
                    }
                )
                pywebview.api.getDiscordAccounts().then(
                    (account_data) => {
                        setAvailableAccounts(account_data);
                    }
                )
            }
        }
        async function updateConsole() {
            while (true) {
                await new Promise(resolve => setTimeout(resolve, 2500));
                pywebview.api.getState().then(
                    (result) => {
                        result = result + 1;
                        let res = result.substring(0, result.length - 1);
                        let connectionState = document.querySelector("span.console-connection-info");
                        connectionState.innerHTML = res;
                        if (res == "Connected") {
                            connectionState.style.color = "#30d64c";
                        }
                    })
                pywebview.api.getAccount().then(
                    (result) => {
                        result = result + 1;
                        let res = result.substring(0, result.length - 1);
                        if (res) {
                            let accountState = document.querySelector("span.console-account-info");
                            accountState.innerHTML = res;
                        }
                    })
                pywebview.api.getPrefix().then(
                    (result) => {
                        result = result + 1;
                        let res = result.substring(0, result.length - 1);
                        if (res) {
                            // console
                            let sbPrefix = document.querySelector("span.console-selfbot-prefix");
                            sbPrefix.innerHTML = res;
                            // info
                            let infoprefix = document.getElementById("selfbot_prefix");
                            infoprefix.innerHTML = res;
                        }
                    })
            }
        }
        document.getElementById("discord_invite").addEventListener("click", function() {
            pywebview.api.getDiscordInvite().then(
                (result) => {
                    pywebview.api.discordJump(result);
                }
            );
        });

        async function updateUptime(){
            while (true) {
            await new Promise(resolve => setTimeout(resolve, 1000));
                pywebview.api.getUptime().then(
                    (uptime) => {
                        uptime = uptime + 1;
                        let new_uptime = uptime.substring(0, uptime.length - 1);
                        let sbUptime = document.getElementById("selfbot_uptime");
                        sbUptime.innerHTML = new_uptime;
                    }
                )
                pywebview.api.getTotalUptime().then(
                    (total_uptime) => {
                        total_uptime = total_uptime + 1;
                        let new_total_uptime = total_uptime.substring(0, total_uptime.length - 1);
                        let sbTotalUptime = document.getElementById("sb_total_uptime");
                        sbTotalUptime.innerHTML = new_total_uptime;
                    }
                )
            }
        }
        async function updateInfo() {
            while (true) {
                await new Promise(resolve => setTimeout(resolve, 30000));
                pywebview.api.getCurrentVersion().then(
                    (result) => {
                        result = result + 1;
                        let res = result.substring(0, result.length - 1);
                        if (res) {
                            let infoprefix = document.getElementById("selfbot_version");
                            infoprefix.innerHTML = res;
                        }
                    })
                    
                    pywebview.api.getGlobalLogins().then(
                    (result) => {
                        result = result + 1;
                        let res = result.substring(0, result.length - 1);
                        if (res) {
                            let infoprefix = document.getElementById("global_logins_count");
                            infoprefix.innerHTML = res;
                        }
                    })
                    pywebview.api.getGlobalUsers().then(
                    (result) => {
                        result = result + 1;
                        let res = result.substring(0, result.length - 1);
                        if (res) {
                            let infoprefix = document.getElementById("global_users_count");
                            infoprefix.innerHTML = res;
                        }
                    })
                    // pywebview.api.getMotd().then(
                    // (result) => {
                    //     result = result + 1;
                    //     let res = result.substring(0, result.length - 1);
                    //     if (res) {
                    //         let infoprefix = document.getElementById("info-motd");
                    //         infoprefix.innerHTML = res;
                    //     }
                    // })
            }
        }
        let users_avatar_url;
        async function updateAvatar(){
            while (true) {
            await new Promise(resolve => setTimeout(resolve, 25000));
            pywebview.api.getUserAvatarUrl().then(
                    (img) => {
                        if (users_avatar_url != img) {
                            users_avatar_url = img;
                            setUserImage(users_avatar_url)
                        }
                    }
                )
            }
        }

        async function updateUserInfo(){
            while (true) {
            await new Promise(resolve => setTimeout(resolve, 3000));
            pywebview.api.getInfoData().then(
                (data) => {
                    if (data["userStatus"] !== "unknown") {
                    if (data["userStatus"] == "invisible"){
                        data["userStatus"] = "offline"
                    }
                    switchUserStatus(data["userStatus"]);
                    updateUserName(data["userName"], data["userDiscriminator"]);
                    updateUserBadges(...data["userBadges"]);
                    }
                }
            )
            }
        }
        async function updateGraphsCount(){
            while (true) {
            await new Promise(resolve => setTimeout(resolve, 30000));
            pywebview.api.getInfoData().then(
                (data) => {
                        updateGraphsAndCountAnimations({serverCount: data["userServers"], maxServerCount: data["maxUserServers"], friendsCount: data["userFriends"], maxFriendsCount: data["maxUserFriends"]});
                    }
            )
            }
        }
        // let connected_nighty = false;
        // async function updateProfileCard() {
        //     //setCardLoading(userCard, true);
