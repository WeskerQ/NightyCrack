        // Usage: toggleSidebar()
        // Optional arg can be specified if needed, to force close or open
        function toggleSidebar(showSidebar = undefined) {
            const isSidebarEnabled = document.body.dataset.sidebarEnabled === 'true';

            if(showSidebar === true) {
            document.body.dataset.sidebarEnabled = true;
            return;
            }

            if(showSidebar === false) {
            document.body.dataset.sidebarEnabled = false;
            return;
            }

            document.body.dataset.sidebarEnabled = !isSidebarEnabled;
        };

        //var intervalID = window.setInterval(update, 1);

        ///////////////////////////////////////////////////////////////////////////////////

        /*
            Update available accounts to switch. If no accounts available,
            Note 1: profile names must be unique
            Note 2: It is the best to provide first 30 characters of the token

            Example usage:
            setAvailableAccounts([
            {
            profileName: 'myprofile123',
            discordToken: 'Mfa.1298hjrf9028hg42890h.gr8904uj21'
            },
            ])

        */
        function setAvailableAccounts(accounts = [
            {
            profileName: '',
            discordToken: ''
            }
        ]) {
            dialogData['SWITCH_ACC'].accountsData = accounts;

            if(accounts.length === 0) {
            document.getElementById('switch-account-button').setAttribute('disabled', '')
            } else {
            document.getElementById('switch-account-button').removeAttribute('disabled')
            }
        }

        // setting to 0 initially
        setAvailableAccounts([]);

        /*
            Custom event: switchaccs
            Events get fired when user selects a profile to switch and clicks the "Switch!" button
        */
        window.addEventListener('switchaccs', (e) => {
            const selectedProfile = e.detail;
            pywebview.api.switchAccount(selectedProfile).then(
                (response) => {
                    if (response === "Already logged in") {
                        window.dispatchEvent(new CustomEvent('switchaccsfailed', {detail: {title: response, message: "Selected account is already connected."}}))
                    }
                }
            )
            // switch acc and reload nighty code here


            /* 
            In case switching accs fails before nighty is reloaded, dispatch the event to show an error message to the user

            Example usage:

            const errorMessage = {
                title: 'An error occured',
                message: 'Selected account is not vaild. Please try again later.'
            }

            window.dispatchEvent(new CustomEvent('switchaccsfailed', {detail: errorMessage}))
            */
        })


        ////////////////////////////////////////////////////////////////////////

        /*
            Custom event: addnewacc
            Events get fired when user adds a new account.
        */
        window.addEventListener('addnewacc', async (e) => {
            const {profileName, token, password} = e.detail;
            pywebview.api.addAccount(profileName, token, password).then(
            (response) => {
                if (response === "Account added"){
                pywebview.api.getDiscordAccounts().then(
                    (info_data) => {
                    setAvailableAccounts(info_data);
                    window.dispatchEvent(new CustomEvent('addnewaccsuccess'));
                    }
                )
                } else if (response === "Profile name already exists") {
                window.dispatchEvent(new CustomEvent('addnewaccerror', {detail: response}));
                } else if (response.includes("Token already saved in profile")) {
                window.dispatchEvent(new CustomEvent('addnewaccerror', {detail: response}));
                } else {
                window.dispatchEvent(new CustomEvent('addnewaccerror', {detail: "Failed to add account."}));
                }
            }
            );
            // Handle adding account here

            /*
            // ON SUCCESS:
            window.dispatchEvent(new CustomEvent('addnewaccsuccess'));

            // update available accounts

            setAvailableAccounts([
                {
                profileName,
                discordToken: token
                },
                // all accounts go here
            ])

            ////////////////////////////////////////////////////////

            // ON ERROR:
            const errorMessage = 'Something went wrong.'
            window.dispatchEvent(new CustomEvent('addnewaccerror', {detail: errorMessage}));
            */
        })

        /*
            Custom event: deleteacc
            Events get fired when user deletes an account.
        */
        window.addEventListener('deleteacc', async (e) => {
            const profileName = e.detail;
            pywebview.api.deleteAccount(profileName).then(
            (response) => {
                if (response === "Account deleted") {
                const updatedAvailableAccounts = dialogData['SWITCH_ACC'].accountsData.filter(acc => acc.profileName !== profileName);
                setAvailableAccounts(updatedAvailableAccounts);
                window.dispatchEvent(new CustomEvent('deleteaccsuccess'));
                } else {
                window.dispatchEvent(new CustomEvent('deleteaccfailed', {detail: response}));
                }
            }
            );
            // on success:
            /*
                1. Update set available accounts. You can either do it manually by getting data from json and calling setAvailableAccounts([]) or use the snippet below

                const updatedAvailableAccounts = dialogData['SWITCH_ACC'].accountsData.filter(acc => acc.profileName !== profileName);
                setAvailableAccounts(updatedAvailableAccounts);

                2. dispatch the event
                window.dispatchEvent(new CustomEvent('deleteaccsuccess'));
            */

            // on error:
            /*
            const errorMessage = 'Something went wrong.'
            window.dispatchEvent(new CustomEvent('deleteaccfailed', {detail: errorMessage}))
            */

        })

        
